{
	"incremental": false,
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/BMO/ElasticSearch"
		}
	},
	"source": {
		"host": "http://elasticsearch1.bugs.scl3.mozilla.com",
		"port": 9200,
		"index": "public_bugs",
		"type": "bug_version",
		"debug": false
	},
	"destination": {
		"host": "http://elasticsearch1.bugs.scl3.mozilla.com",
		"port": 9200,
		"index": "bug_hierarchy",
		"type": "bug_hierarchy",
		"debug": false,
		"read_only": false
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			},
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "./results/logs/hierarchy_etl.log",
				"maxBytes": 10000000,
				"backupCount": 3,
				"encoding": "utf8"
			},
			{
				"subject": "[ALERT][Toronto]Problem with public hierarchy",
				"$ref": "email.json"
			}

		]
	}
}
