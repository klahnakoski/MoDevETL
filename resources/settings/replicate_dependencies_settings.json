{
	"source": {
		"host": "https://esfrontline.bugzilla.mozilla.org",
		"port": 443,
		"index": "public_bugs",
		"type": "bug_version",
		"explore_metadata": false
	},
	"destination": {
		"host": "http://localhost",
		"port": 9200,
		"index": "bug_hierarchy",
		"type": "bug_version"
	},
	"param": {
		"last_replication_time": "./results/data/last_dependencies_replication_time.txt"
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"subject": "[ALERT][STAGING]Problem with ActiveData Server",
				"$ref": "email.json"
			},
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			}
		]
	}

}
